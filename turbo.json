{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["NODE_ENV"]
    },
    "dev": {
      "cache": false
    },
    "build:docs": {
      "dependsOn": ["^build"],
      "outputs": ["docs/.island/dist/**"]
    }
  }
}
